{
    "extends": "airbnb-base",
    "rules": {
        "indent": ["error", 4],
        "comma-dangle": ["error", "never"],
        "camelcase": ["off", {"properties":"never"}],
        "object-curly-newline": ["error", {"minProperties": 7}],
        "no-confusing-arrow": ["off"],
        "no-param-reassign": ["error", {"props": false}],
        "max-len": ["error", 150]
    }
}
